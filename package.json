{
  "name": "my-scheduler-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:cron": "ENABLE_CRON_JOBS=true next dev",
    "dev:cluster": "REDIS_USE_CLUSTER=true ENABLE_CRON_JOBS=true ENABLE_SYSTEM_MONITORING=true ENABLE_AUTO_SCALING=true next dev",
    "dev:auto": "scripts/start-with-cron.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:cron": "npx tsx scripts/debug/test-cron.ts",
    "test:bullmq": "npx tsx scripts/debug/test-bullmq.ts",
    "test:cluster": "npx tsx scripts/debug/test-cluster.ts",
    "test:integration": "npx tsx scripts/debug/test-integration.ts",
    "test:facebook": "npx tsx scripts/debug/test-facebook-api.ts",
    "test:instagram": "npx tsx scripts/debug/test-instagram-api.ts",
    "facebook:get-page-tokens": "npx tsx scripts/get-page-token.ts",
    "instagram:get-accounts": "npx tsx scripts/get-instagram-accounts.ts",
    "instagram:validate": "npx tsx scripts/get-instagram-accounts.ts --validate",
    "seed:hashtags": "npx tsx src/features/social/scripts/seed-hashtags.ts",
    "seed:permissions": "npx tsx src/server/permissions/seed/seed-role-permissions.ts",
    "analytics:collect": "npx tsx scripts/collect-analytics.ts",
    "analytics:check": "npx tsx scripts/debug/check-analytics.ts",
    "test:analytics": "npx tsx scripts/test-analytics-frontend.ts",
    "test:rich-insights": "npx tsx scripts/test-rich-insights.ts",
    "test:api-endpoint": "npx tsx scripts/test-api-endpoint.ts",
    "create:mock-analytics": "npx tsx scripts/create-mock-analytics.ts",
    "debug:rawinsights": "npx tsx scripts/debug-rawinsights.ts",
    "test:publish-platform-id": "tsx scripts/debug/test-publish-with-platform-id.ts",
    "test:create-and-publish": "tsx scripts/create-and-test-publish.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "cron:restart": "curl -X POST localhost:3000/api/cron-manager -H 'Content-Type: application/json' -d '{\"action\": \"restart\", \"apiKey\": \"test-scheduler-key\"}'",
    "cron:status": "curl 'localhost:3000/api/cron-manager?action=status&apiKey=test-scheduler-key'",
    "cron:stats": "curl 'localhost:3000/api/cron-manager?action=stats&apiKey=test-scheduler-key'",
    "cron:monitor": "npx tsx scripts/monitor-cron-performance.ts",
    "cron:watch": "while true; do npm run cron:status && sleep 30; done",
    "cron:stop": "curl -X POST localhost:3000/api/cron-manager -H 'Content-Type: application/json' -d '{\"action\": \"stop\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:init": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"initialize\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:status": "curl 'localhost:3000/api/queue-manager?action=status&apiKey=test-scheduler-key'",
    "queue:metrics": "curl 'localhost:3000/api/queue-manager?action=queue_metrics&apiKey=test-scheduler-key'",
    "queue:available": "curl 'localhost:3000/api/queue-manager?action=available_queues&apiKey=test-scheduler-key'",
    "queue:pause": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"pause_queue\", \"queueName\": \"scheduler\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:resume": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"resume_queue\", \"queueName\": \"scheduler\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:clean": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"clean_queue\", \"queueName\": \"scheduler\", \"cleanType\": \"completed\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:test-post": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"schedule_post_publish\", \"postId\": \"test_123\", \"platform\": \"facebook\", \"userId\": \"test_user\", \"content\": {\"text\": \"Test post from BullMQ\"}, \"apiKey\": \"test-scheduler-key\"}'",
    "queue:test-health": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"schedule_health_check\", \"checkType\": \"quick\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:test-notification": "curl -X POST localhost:3000/api/queue-manager -H 'Content-Type: application/json' -d '{\"action\": \"schedule_notification\", \"userId\": \"test_user\", \"type\": \"email\", \"template\": \"test_template\", \"apiKey\": \"test-scheduler-key\"}'",
    "queue:monitor": "node scripts/monitor-queues.js",
    "cluster:setup": "chmod +x scripts/setup-redis-cluster.sh && ./scripts/setup-redis-cluster.sh setup",
    "cluster:start": "./scripts/setup-redis-cluster.sh start",
    "cluster:stop": "./scripts/setup-redis-cluster.sh stop",
    "cluster:restart": "./scripts/setup-redis-cluster.sh restart",
    "cluster:status": "./scripts/setup-redis-cluster.sh status",
    "cluster:test": "./scripts/setup-redis-cluster.sh test",
    "cluster:clean": "./scripts/setup-redis-cluster.sh clean",
    "cluster:info": "./scripts/setup-redis-cluster.sh info",
    "monitor:start": "curl -X POST localhost:3000/api/monitoring -H 'Content-Type: application/json' -d '{\"action\": \"start_monitoring\", \"intervalMinutes\": 5, \"apiKey\": \"test-scheduler-key\"}'",
    "monitor:stop": "curl -X POST localhost:3000/api/monitoring -H 'Content-Type: application/json' -d '{\"action\": \"stop_monitoring\", \"apiKey\": \"test-scheduler-key\"}'",
    "monitor:status": "curl 'localhost:3000/api/monitoring?action=monitoring_status&apiKey=test-scheduler-key'",
    "monitor:health": "curl 'localhost:3000/api/monitoring?action=health_check&apiKey=test-scheduler-key'",
    "monitor:system": "curl 'localhost:3000/api/monitoring?action=system_metrics&apiKey=test-scheduler-key'",
    "monitor:redis": "curl 'localhost:3000/api/monitoring?action=redis_cluster&apiKey=test-scheduler-key'",
    "monitor:queues": "curl 'localhost:3000/api/monitoring?action=queue_metrics&apiKey=test-scheduler-key'",
    "monitor:watch": "watch -n 10 'curl -s localhost:3000/api/monitoring?action=health_check&apiKey=test-scheduler-key | jq'",
    "scaling:start": "curl -X POST localhost:3000/api/monitoring -H 'Content-Type: application/json' -d '{\"action\": \"start_autoscaling\", \"intervalMinutes\": 2, \"apiKey\": \"test-scheduler-key\"}'",
    "scaling:stop": "curl -X POST localhost:3000/api/monitoring -H 'Content-Type: application/json' -d '{\"action\": \"stop_autoscaling\", \"apiKey\": \"test-scheduler-key\"}'",
    "scaling:status": "curl 'localhost:3000/api/monitoring?action=scaling_status&apiKey=test-scheduler-key'",
    "scaling:metrics": "curl 'localhost:3000/api/monitoring?action=scaling_metrics&apiKey=test-scheduler-key'",
    "scaling:manual": "curl -X POST localhost:3000/api/monitoring -H 'Content-Type: application/json' -d '{\"action\": \"manual_scale\", \"queueName\": \"notifications\", \"targetWorkers\": 10, \"reason\": \"Manual scaling test\", \"apiKey\": \"test-scheduler-key\"}'",
    "scaling:config": "curl -X POST localhost:3000/api/monitoring -H 'Content-Type: application/json' -d '{\"action\": \"update_scaling_config\", \"queueName\": \"high-priority\", \"maxWorkers\": 15, \"targetWaitingJobs\": 8, \"apiKey\": \"test-scheduler-key\"}'",
    "dashboard": "echo 'Opening monitoring dashboard...' && open 'http://localhost:3000/api/monitoring?apiKey=test-scheduler-key' || echo 'Visit: http://localhost:3000/api/monitoring?apiKey=test-scheduler-key'"
  },
